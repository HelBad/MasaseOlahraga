                        android:textStyle="bold"/>

                    <WebView
                        android:id="@+id/viewMateriPertolongan9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:ignore="WebViewLayout" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>