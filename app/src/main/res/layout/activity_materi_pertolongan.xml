                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="5)"
                        android:textSize="15dp"
                        android:textColor="@color/gray"
                        android:textStyle="bold"/>

