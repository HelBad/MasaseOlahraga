                        tools:ignore="WebViewLayout" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutMateriPertolongan7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="6)"
                        android:textSize="15dp"
                        android:textColor="@color/gray"
                        android:textStyle="bold"/>

                    <WebView
                        android:id="@+id/viewMateriPertolongan7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:ignore="WebViewLayout" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutMateriPertolongan8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="7)"
                        android:textSize="15dp"
                        android:textColor="@color/gray"
                        android:textStyle="bold"/>

                    <WebView
                        android:id="@+id/viewMateriPertolongan8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:ignore="WebViewLayout" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutMateriPertolongan9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="8)"
