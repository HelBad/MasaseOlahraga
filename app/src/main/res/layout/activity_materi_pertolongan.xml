<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical"
    tools:context=".view.cedera.materi.ActivityPertolongan">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarMateriPertolongan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:minHeight="?attr/actionBarSize">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Cedera Dan Pertolongan Pertama"
            android:textColor="@android:color/black"
            android:textSize="18dp"
            android:textStyle="bold" />
    </androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:orientation="vertical">
                <WebView
                    android:id="@+id/viewMateriPertolongan1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="WebViewLayout" />

                <LinearLayout
                    android:id="@+id/layoutMateriPertolongan2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="1)"
                        android:textSize="15dp"
                        android:textColor="@color/gray"
                        android:textStyle="bold"/>

                    <WebView
                        android:id="@+id/viewMateriPertolongan2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:ignore="WebViewLayout" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutMateriPertolongan3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="2)"
                        android:textSize="15dp"
                        android:textColor="@color/gray"
                        android:textStyle="bold"/>

                    <WebView
                        android:id="@+id/viewMateriPertolongan3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:ignore="WebViewLayout" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutMateriPertolongan4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="3)"
                        android:textSize="15dp"
                        android:textColor="@color/gray"
                        android:textStyle="bold"/>

                    <WebView
                        android:id="@+id/viewMateriPertolongan4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:ignore="WebViewLayout" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutMateriPertolongan5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="15dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="5dp"
                        android:text="4)"
